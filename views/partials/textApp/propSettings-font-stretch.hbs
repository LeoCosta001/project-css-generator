<!-- [Propriedade "font-stretch"] -->
<div class="prop__container__font-stretch">

    <!-- [Titulo] de "font-stretch"-->
    <div class="prop__title title__font-stretch no-select">
        <p>Expanção e Retração</p>
        <hr>
        <div>
            <p>font-stretch</p>
        </div>
    </div>
    <div class="content__font-stretch">

        <!-- [Tipo de valor de "font-stretch"] -->
        <div class="input__type__select">
            <input type="button" id="button-predefined-value-font-stretch" class="prop__input__type__button input--type--button--active"
                value="Pré-definido">
        </div>

        <!-- [Input de Valor "Pré-Definido" de "font-stretch"] -->
        <div class="predefined__value__container__font-stretch">
            <p class="input__title no-select">Expanção dos caracteres</p>
            <label class="mini__arrow"></label>
            <select id="property-font-stretch-value-predefined" class="prop__select__color">
                <option value="normal">normal</option>
                <option value="ultra-condensed">ultra-condensed</option>
                <option value="extra-condensed">extra-condensed</option>
                <option value="condensed">condensed</option>
                <option value="semi-expanded">semi-expanded</option>
                <option value="expanded">expanded</option>
                <option value="extra-expanded">extra-expanded</option>
                <option value="ultra-expanded">ultra-expanded</option>
            </select>
        </div>
    </div>
</div>